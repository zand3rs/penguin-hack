<div class="wrap">
  <a href="/" class="penguin-logo">Penguin CMS</a>
  <h1 class="logo-title">Penguin</h1>
  <h1 class="app-title">Sign in as Guest</h1>
  <div id="userGuest" class="dropdown-login">
    <select class="select-login">
      <% _.forEach(users, function(user){ %>
        <option value="<%- user.id %>"><%- user.displayName %></option>
      <%})%>
    </select>
    <span class="ico ico-arrow-down3 icon-select"></span>
  </div><br>
  <a href="#" class="cta-button" id="cta-button" onclick='getAuthUser()'>Sign in</a>
</div>

<script src="/js/lib/jquery.js"></script>
<script type="text/javascript">
  function getAuthUser()
  {
    var newUserUrl = $( "#userGuest option:selected" ).val();
    $("#cta-button").attr("href", 'signin' + '/' + newUserUrl);

  }

</script>
